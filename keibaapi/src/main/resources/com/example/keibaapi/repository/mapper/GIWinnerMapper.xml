<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.example.keibaapi.repository.mapper.GIWinnerMapper">
    <select id="findGIWinner" resultType="com.example.keibaapi.repository.entity.GIWinner">
        SELECT  year,
                race,
                winner
        FROM GI_winner
